{
    "description": "Unlocks Medium articles",
    "manifest_version": 2,
    "name": "Fredium",
    "version": "2.0",
    "icons": {
      "256": "logo.png"
    },

    "permissions": [
        "scripting",
        "activeTab"
    ],
  "content_scripts": [
    {
      "matches": ["http://medium.com/"],
      "js": []
    }
  ],
  
    "background": {
      "scripts": ["fredium.js"]
    },
  
    "browser_action": {
      "default_icon": "logo.png"
    }
}