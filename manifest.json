{
    "description": "Unlocks Medium articles",
    "manifest_version": 2,
    "name": "Fredium",
    "version": "1.0",
    "icons": {
      "256": "logo.png"
    },

    "permissions": [
        "tabs",
        "scripting",
        "activeTab"
    ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": []
    }
  ],
  
    "background": {
      "scripts": ["fredium.js"]
    },
  
    "browser_action": {
      "default_icon": "logo.png"
    }
}